<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intro.android.smartcity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartCity">
        <activity android:name=".Login"></activity>
        <activity android:name=".Problema_List" />
        <activity android:name=".NotasPessoais.NotasPessoais" />
        <activity android:name=".NotasPessoais.AddNotas" />
        <activity android:name=".NotasPessoais.EditNotas" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>